	<body>
    <div id="pagewrap">
     
      <% // 整个页面的头部 start %>
      <div id="header">
        <div class="header-wrap">
          <div id="site-logo" class="site-logo">
            <h1><a href="/">音韵码工</a></h1>
          </div>
        <div id="site-description" classs="site-description">专注前端，node.js，<span id="flashLite"></span></div>
        </div>
      </div>
      <% // 整个页面的头部 end %>
      
      <% // 头部的导航栏 start %>
      <div id="navigator" class="navigator">
        <div class="navigator-wrap">
          <ul>
            <li class="active item">
              <a href="javascript:;">首页</a>
            </li>
            <script>
            (function() {
              var nav = document.getElementById('navigator');
              var tabs = nav.getElementsByTagName('li');
              var tabsLen = tabs.length;
              var _hasAct = false;
              for(var i=0; i<tabsLen; i++) {
                if(tabs[i].className.indexOf('active') != -1) {
                  _hasAct = true;
                  break;
                }
              }
              
              if(!_hasAct) {
                tabs[0].className += ' active'
              }
            })();
            </script>
          </ul>
        </div>
      </div>
      <% // 头部的导航栏 end %>
              
      <div id="body">
        <div class="body-wrap clearfix" style="height: 900px;">